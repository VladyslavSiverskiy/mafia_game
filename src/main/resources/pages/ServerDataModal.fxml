    <StackPane fx:id="loadingPane" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
    <children>

            <Label fx:id="warningLabel" layoutX="14.0" layoutY="86.0" />
            <Button fx:id="import_btn" layoutX="57.0" layoutY="73.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="189.0" text="Зберегти дані на сервері" textFill="#0c0c0c" />
      <Button fx:id="export_btn" layoutX="57.0" layoutY="134.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="189.0" text="Завантажити дані з серверу" />
        </children>
    <Pane fx:id="overlayPane" style="-fx-background-color: rgba(0, 0, 0, 0.5);" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />

</AnchorPane>
